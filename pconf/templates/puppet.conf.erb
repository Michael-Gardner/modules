[main]
  logdir = /var/log/puppet
  rundir = /var/run/puppet
  ssldir = $vardir/ssl
  reportdir = $vardir/reports
  server = <%= @servername %>
  environmentpath = $confdir/environments
  basemodulepath = <%= @basemodulepath %>
  pluginsync = <%= @pluginsync.to_s %>
  archive_files = <%= @archive_files.to_s %>
  archive_file_server = <%= @archive_file_server %>
  log_level = <%= @log_level %>
  stringify_facts = <%= @stringify_facts.to_s %>

[agent]
  environment = <%= @environment %>
  runinterval = <%= @runinterval %>
  report      = <%= @report %>

[master]
  autosign             = <%= @autosign.to_s %>
	reports              = <%= @reports.to_s %>
	reporturl            = <%= @reporturl %>
  storeconfigs         = <%= @storeconfigs.to_s %>
  storeconfigs_backend = <%= @storeconfigs_backend %>
